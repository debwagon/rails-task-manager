<h1>Tasks</h1>
<ul>
<% @tasks.each do |task| %>
  <li><%= task.title %>
    <%= link_to "details", task_path(:id => task.id) %>
    <%= link_to "edit", edit_path(:id => task.id) %>
    <%= link_to "delete", task_path(:id => task.id), method: :delete %>
  </li>
  <% end %>
  <%= link_to "new", new_path %>
</ul>
